{
	"schemaVersion": 1,
	"id": "chatzone",
	"version": "${version}",
	"name": "ChatZone",
	"description": "Makes chatting via a command (like continous /msgs) easy!",
	"authors": [
		"TheMasterKitty"
	],
	"icon": "assets/chatzone/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"themasterkitty.chatzone.Main"
		]
	},
	"mixins": [
		{
			"config": "chatzone.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": "*",
		"minecraft": [ "1.20.x", "1.21.x" ],
		"java": ">=17",
		"fabric-api": "*"
	}
}